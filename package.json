{
  "name": "j2c",
  "version": "0.7.2",
  "description": "A JavaScript object to CSS compiler.",
  "main": "dist/j2c.commonjs.js",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node_modules/mocha/bin/mocha --reporter nyan --ui qunit",
    "cover": "node_modules/mocha/bin/mocha --require blanket --ui qunit --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pygy/j2c.git"
  },
  "keywords": ["CSS", "compiler", "preprocessor", "SASS", "LESS", "Stylus", "JSON"],
  "author": "Pierre-Yves GÃ©rardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pygy/j2c/issues"
  },
  "config": {
    "blanket": {
      "pattern": "//j2c\\.commonjs/"
    }
  },
  "testling": {
    "harness": "mocha-qunit",
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  },
  "homepage": "http://j2c.py.gy",
  "devDependencies":{
    "uglify-js":"*",
    "crass":"*",
    "mocha":"*",
    "expect.js":"*",
    "surgicate":"*",
    "coveralls": "^2.11.2",
    "mocha-lcov-reporter": "0.0.2",
    "blanket": "^1.1.7"
  }
}